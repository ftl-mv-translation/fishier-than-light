</mod:findName>

<mod:findName type="event" name="COMBAT_CHECK_HOTKEYS">
  <mod-append:choice hidden="true">
    <text>Fishing Button (default is the tab key)</text>
		<event name="COMBAT_CHECK_HOTKEYS_FISH_START">
      <text>Press any key and use the mouse to select "Continue" to set a new key, or select "Restore Default" to return this hotkey to its default setting.</text>
      <choice hidden="true">
          <text>Continue</text>
          <event name="COMBAT_CHECK_HOTKEYS_FISH_END_1">
              <loadEvent>COMBAT_CHECK_TOGGLE</loadEvent>
          </event>
      </choice>
      <choice hidden="true">
          <text>Restore Default</text>
          <event name="COMBAT_CHECK_HOTKEYS_FISH_END_2">
              <metaVariable name="prof_hotkey_fish" op="set" val="30" />
              <loadEvent>COMBAT_CHECK_TOGGLE</loadEvent>
          </event>
      </choice>
    </event>
  </mod-append:choice>
</mod:findName>

