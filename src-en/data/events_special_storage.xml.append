<FTL>
<mod:findName type="event" name="STORAGE_CHECK_BUTTON">
  <mod-append:choice req="fish_option" blue="true" hidden="true">
    <text>Fishing.</text>
    <event load="STORAGE_CHECK_FISHING" />
  </mod-append:choice>
  <mod-append:choice req="MAW_BLUELIST_ALL" blue="true" hidden="true">
